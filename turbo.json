{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {},
    "test": {},
    "test:watch": {
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    }
  }
}